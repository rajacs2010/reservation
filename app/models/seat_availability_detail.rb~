class SeatAvailabilityDetail < ActiveRecord::Base
    attr_accessible :train_number, :travel_date, :first_ac, :second_ac, :third_ac, :chair_car, :sleeper, :sitting
end  #class SeatAvailabilityDetail < ActiveRecord::Base
